{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": 0,
    "class-methods-use-this": "off",
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "prop-types": [0],
    "import/no-unresolved": "off"
  },
  "extends": ["airbnb-base"]
}
